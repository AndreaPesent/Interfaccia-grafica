<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="462.0" prefWidth="537.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.example.interfacciagrafica.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox prefHeight="48.0" prefWidth="497.0">
      <children>
         <Button fx:id="Aggiungi" mnemonicParsing="false" text="Aggiunta " />
         <Button fx:id="Visualizza" mnemonicParsing="false" text="Visualizza" />
         <Button fx:id="Modifica" mnemonicParsing="false" text="Modifica" />
         <Button fx:id="Cancella" mnemonicParsing="false" text="Cancella" />
         <Button fx:id="Ngol" mnemonicParsing="false" text="5+ gol" />
         <Button fx:id="Capitano" mnemonicParsing="false" text="Capitano" />
      </children>
   </HBox>
   <HBox prefHeight="85.0" prefWidth="497.0">
      <children>
         <TextField fx:id="Nome" />
         <TextField fx:id="NumGol" />
         <CheckBox fx:id="IsCapitano" mnemonicParsing="false" text="Capitano" />
      </children>
   </HBox>
   <TextArea fx:id="Output" prefHeight="200.0" prefWidth="200.0" />
</VBox>
